{
  "expo": {
    "name": "Assistant",
    "slug": "Assistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "your-app-scheme",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Assistant",
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app requires location access",
        "NSContactsUsageDescription": "This app requires access to contacts to send birthday messages",
        "LSApplicationQueriesSchemes": [
          "sms"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.Assistant"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Jersey15-Regular.ttf",
            "./assets/fonts/Coiny-Regular.ttf"
          ],
          "ios": {
            "fonts": [
              "./assets/fonts/Jersey15-Regular.ttf",
              "./assets/fonts/Coiny-Regular.ttf"
            ]
          }
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow planner to access your contacts."
        }
      ],
      "expo-router",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ]
    ]
  }
}